    "cheerio": "^1.1.2",
    "dotenv": "^17.2.3",
    "hono": "^4.10.0",
    "lru-cache": "^11.2.2",
    "music-metadata": "^11.10.0",
    "prisma": "7.0.0",
    "redis": "^5.9.0",
    "ts-pattern": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
